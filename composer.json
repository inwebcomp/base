{
    "name": "inweb/base",
    "description": "InWeb base components and helpers",
    "keywords": [
        "framework",
        "laravel",
        "inweb"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Topalo",
            "email": "inwebcomp@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/chrispage1/laravel-nestedset"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "spatie/once": "^2.0",
        "cviebrock/eloquent-sluggable": "^7.0",
        "astrotomic/laravel-translatable": "^11.0",
        "kalnoy/nestedset": "^5.0",
        "spatie/eloquent-sortable": "^3.7"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "InWeb\\Base\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InWeb\\Base\\Database\\Seeds\\": "src/database/seeds/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "InWeb\\Base\\Providers\\AppServiceProvider"
            ]
        }
    }
}
